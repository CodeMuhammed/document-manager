    "babel-preset-react": "^6.16.0",
    "babel-watch": "^2.0.4",
    "css-loader": "^0.26.1",
    "eslint-plugin-react": "^6.8.0",
    "istanbul": "^0.4.5",
    "jasmine": "^2.5.2",
    "node-sass": "^4.1.1",
    "sass-loader": "^4.1.1",
    "serve": "^2.1.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0"
  }
