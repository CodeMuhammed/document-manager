{
  "name": "sequilize",
  "version": "1.0.0",
  "description": "Document manager CP-3",
  "main": "index.js",
  "scripts": {
    "app": "npm run server &  npm run client:watch",
    "server": "node_modules/.bin/babel-watch ./server/api",
    "test": "npm run reset && node_modules/.bin/babel-istanbul cover node_modules/.bin/jasmine",
    "coveralls": "cat ./coverage/*.info | ./node_modules/coveralls/bin/coveralls.js",
    "reset": "npm run migration:gen && npm run migration:db && npm run seed:clear && npm run seed:insert",
    "migration:gen": "node_modules/.bin/babel-node ./server/helpers/migration-file-Generator.js",
    "migration:db": "./node_modules/.bin/sequelize db:migrate",
    "seed:insert": "./node_modules/.bin/sequelize db:seed:all",
    "seed:clear": "./node_modules/.bin/sequelize db:seed:undo:all",
    "seed:create": "./node_modules/.bin/sequelize seed:create",
    "client:watch": "webpack -d --watch",
    "client:build": "webpack"
  },
  "keywords": [
    "document",
    "manager",
    "postgres"
  ],
  "author": "muhammed ali",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-istanbul": "^0.12.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "coveralls": "^2.11.15",
    "eslint": "^3.9.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "express": "^4.14.0",
    "fs-extra": "^1.0.0",
    "i": "^0.3.5",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.2.1",
    "materialize-css": "^0.97.8",
    "method-override": "^2.3.7",
    "morgan": "^1.7.0",
    "mysql": "^2.12.0",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-select": "^1.0.0-rc.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
